sudo docker stop my-mysql
sudo docker rm my-mysql
mkdir data
sudo docker run -p 33060:3306 --name my-mysql -e MYSQL_ROOT_PASSWORD=secret -v "$PWD/data":/var/lib/mysql -d mysql:8.0
sleep 20
sudo docker exec -it my-mysql mysql -p secret data
CREATE DATABASE check;
USE check;
CREATE TABLE accounts (
    id INT AUTO_INCREMENT PRIMARY KEY,
    username VARCHAR(50) NOT NULL,
    password VARCHAR(255) NOT NULL,
    email VARCHAR(100) NOT NULL
);
INSERT INTO accounts (username, password, email)
VALUES ('test', 'test123', 'test@example.com');